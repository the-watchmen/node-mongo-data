    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-0": "6.24.1",
    "cross-env": "5.1.3",
    "eslint": "4.15.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.4.0",
